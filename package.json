{
  "name": "aspire-components-react",
  "description": "Aspire Components for React",
  "repository": "https://github.com/chrstnfrrs/aspire-components",
  "author": "chrstnfrrs",
  "license": "MIT",
  "private": false,
  "version": "0.0.0",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build"
  ],
  "source": "src/index.js",
  "engines": {
    "node": ">=12"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "scripts": {
    "build": "rollup -c",
    "build:clean": "rm -rf build/ && rollup -c",
    "coverage": "jest --config='test/unit/jest.config.json' --collectCoverage",
    "lint": "eslint './**/*.ts' --ignore-pattern '/node_modules/' --ignore-pattern '/build/' --ignore-pattern '/coverage/'",
    "pre-commit": "yarn prettier",
    "prettier": "prettier --write .",
    "test": "jest --config='jest.config.json'",
    "snapshot": "jest --config='test/snapshot/jest.config.json'",
    "unit": "jest --config='test/unit/jest.config.json'",
    "typeCheck": "tsc -p tsconfig.json",
    "verify": "yarn typeCheck && yarn prettier && yarn lint && yarn coverage && yarn snapshot && yarn build:clean"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/chance": "^1.1.1",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "chance": "^1.1.7",
    "eslint": "^7.22.0",
    "eslint-config-get-off-my-lawn": "^6.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.41.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "aspire-components-functions": "^0.0.3",
    "aspire-components-scss": "^1.0.1",
    "clsx": "^1.1.1"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0"
  }
}
